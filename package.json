{
  "name": "black-angus-bot",
  "version": "1.0.0",
  "description": "Multi-Purpose Discord Bot",
  "main": "src/index.ts",
  "scripts": {
    "start": "ts-node src/index.ts",
    "test": "ava",
    "build": "tsc",
    "lint": "eslint --fix --ext .js,.ts .",
    "clean": "rimraf package-lock.json node-modules && npm install"
  },
  "author": "Haze Lee <hazelee@realignist.me>",
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@semantic-release/changelog": "^5.0.0",
    "@semantic-release/git": "^9.0.0",
    "@types/bluebird": "^3.5.30",
    "@types/charset": "^1.0.1",
    "@types/cheerio": "^0.22.16",
    "@types/debug": "^4.1.5",
    "@types/got": "^9.6.9",
    "@types/lodash": "^4.14.149",
    "@types/mongoose": "^5.7.3",
    "@types/node": "^12",
    "@types/sharp": "^0.24.0",
    "@typescript-eslint/eslint-plugin": "^2.22.0",
    "ava": "^3.5.0",
    "cz-conventional-changelog": "^3.1.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-config-standard-with-typescript": "^14.0.0",
    "eslint-plugin-ava": "^10.2.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^4.2.3",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.2",
    "semantic-release": "^17.0.4",
    "ts-node": "^8.6.2",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "@typegoose/typegoose": "^6.4.0",
    "apng2gif-bin": "^1.7.1",
    "bluebird": "^3.7.2",
    "bufferutil": "^4.0.1",
    "charset": "^1.0.1",
    "cheerio": "^1.0.0-rc.3",
    "date-fns": "^2.10.0",
    "debug": "^4.1.1",
    "discord.js": "^12.0.1",
    "fuzzball": "^1.3.0",
    "got": "^10.6.0",
    "iconv-lite": "^0.5.1",
    "ioredis": "^4.16.0",
    "lodash": "^4.17.15",
    "mongoose": "^5.9.3",
    "rxjs": "^6.5.4",
    "rxjs-compat": "^6.5.4",
    "sharp": "^0.24.1"
  },
  "engines": {
    "node": "=12.x.x",
    "npm": "=6.x.x"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "ava": {
    "compileEnhancements": false,
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
